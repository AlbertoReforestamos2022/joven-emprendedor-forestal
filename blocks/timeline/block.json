      "type": "string",
      "source": "html",
      "selector": ".section-description",
      "default": "Un recorrido por la historia del Joven Emprendedor Forestal"
    },
    "items": {
      "type": "array",
      "default": [
        {
          "year": "2024 - X Edición",
          "titulo": "Innovación y Tecnología Forestal",
          "ubicacion": "Guadalajara, Jalisco",
          "participantes": "800 participantes",
          "descripcion": "Enfoque en drones, inteligencia artificial y herramientas digitales."
        }
      ]
    }
  }
}