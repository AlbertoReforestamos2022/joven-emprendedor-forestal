{
  "apiVersion": 2,
  "name": "jovenemprendedor/timeline",
  "title": "Timeline JEF",
  "category": "jef-theme",
  "icon": "calendar-alt",
  "description": "Línea del tiempo de ediciones",
  "attributes": {
    "titulo": {
      "type": "string",
      "source": "html",
      "selector": "h2",
      "default": "Ediciones Anteriores"
